{
    "name":  "creperie-ozoir",
    "type":  "module",
    "version":  "0.0.0",
    "private":  true,
    "scripts":  {
                    "clean":  "node -e \"try{require('fs').rmSync('dist',{recursive:true,force:true});}catch(e){}\"",
                    "start": "npm run dev:all",
                    "dev:fresh": "npm run clean && npm run dev:all",
                    "build:fresh": "npm run clean && npm run build",
                    "dev":  "vite --config vite.client.config.js",
                    "dev:client":  "vite --config vite.client.config.js",
                    "dev:admin":  "vite --config vite.admin.config.js",
                    "dev:all":  "concurrently -k -n client,admin -c green,cyan \"vite --config vite.client.config.js\" \"vite --config vite.admin.config.js\"",
                    "build":  "npm run build:client && npm run build:admin",
                    "build:client":  "vite build --config vite.client.config.js",
                    "build:admin":  "vite build --config vite.admin.config.js",
                    "preview":  "vite preview --config vite.client.config.js",
                    "preview:client":  "vite preview --config vite.client.config.js --port 5173",
                    "preview:admin":  "vite preview --config vite.admin.config.js --port 5174"
                },
    "dependencies":  {
                         "@radix-ui/react-alert-dialog":  "^1.0.5",
                         "@radix-ui/react-avatar":  "^1.0.3",
                         "@radix-ui/react-checkbox":  "^1.0.4",
                         "@radix-ui/react-dialog":  "^1.0.5",
                         "@radix-ui/react-dropdown-menu":  "^2.0.5",
                         "@radix-ui/react-label":  "^2.0.2",
                         "@radix-ui/react-popover":  "^1.0.7",
                         "@radix-ui/react-select":  "^2.0.0",
                         "@radix-ui/react-slider":  "^1.1.2",
                         "@radix-ui/react-slot":  "^1.0.2",
                         "@radix-ui/react-tabs":  "^1.0.4",
                         "@radix-ui/react-toast":  "^1.1.5",
                         "@supabase/supabase-js":  "^2.56.1",
                         "class-variance-authority":  "^0.7.0",
                         "clsx":  "^2.0.0",
                         "date-fns":  "^2.30.0",
                         "framer-motion":  "^10.16.4",
                         "lucide-react":  "0.292.0",
                         "react":  "^18.2.0",
                         "react-day-picker":  "^8.9.1",
                         "react-dom":  "^18.2.0",
                         "react-helmet":  "^6.1.0",
                         "react-router-dom":  "^6.16.0",
                         "recharts":  "^2.12.7",
                         "tailwind-merge":  "^1.1.4",
                         "tailwindcss-animate":  "^1.0.7"
                     },
